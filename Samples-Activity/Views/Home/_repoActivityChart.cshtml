    <h3 style="position:relative; left:300px; top: 200px; color: rgba(151,187,250,1)">Recent Commit Activity Distribution</h3>
    <canvas id="repoActivityChart" width="1000" height="1000" style="position:relative; left:0px; top: -50px"></canvas>
    <script type="text/javascript">
        var ctx = $("#repoActivityChart").get(0).getContext("2d");
        var names = @Html.Raw(Json.Encode(ViewData["repoNames"]));
        var commitCount = @Json.Encode(ViewData["repoMonthCommitCounts"]);
        console.log(commitCount);
        var data = {
            labels: names,
            datasets: [
                {
                    fillColor: "rgba(151,187,205,0.5)",
                    strokeColor: "rgba(151,187,205,1)",
                    pointColor: "rgba(151,187,205,1)",
                    pointStrokeColor: "#fff",
                    data: commitCount
                }
            ]
        };
        var options = {pointLabelFontSize : 10}
        var myNewChart = new Chart(ctx).Radar(data, options);
    </script>
