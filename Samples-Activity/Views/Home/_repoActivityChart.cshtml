<div>
    <canvas id="repoActivityChart" width="1500" height="1500"></canvas>
    <script type="text/javascript">
        var ctx = $("#repoActivityChart").get(0).getContext("2d");
        var names = @Html.Raw(Json.Encode(ViewData["repoNames"]));
        var commitCount = @Json.Encode(ViewData["repoMonthCommitCounts"]);
        console.log(commitCount);
        var data = {
            labels: names,
            datasets: [
                {
                    fillColor: "rgba(151,187,205,0.5)",
                    strokeColor: "rgba(151,187,205,1)",
                    pointColor: "rgba(151,187,205,1)",
                    pointStrokeColor: "#fff",
                    data: commitCount
                }
            ]
        };
        var myNewChart = new Chart(ctx).Radar(data);
    </script>
</div>
